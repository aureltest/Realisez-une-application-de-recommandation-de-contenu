{
    "version": "2.0",
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.0.0, 5.0.0)"
    },
    "extensions": {
        "blobs": {
            "maxDegreeOfParallelism": 1,
            "poisonBlobThreshold": 1
        }
    },
    "functions": [ "httpTrigger" ],
    "functionTimeout": "00:10:00",
    "logging": {
        "fileLoggingMode": "debugOnly",
        "logLevel": {
          "Function.httpTrigger": "Information",
          "default": "None"
        }
    }
}